    },
    {
      "lineNumber": 560,
      "shift": false,
      "line": {
        "text": "「な、なにおう！？」\r\n"
      }
    },
    {
      "lineNumber": 574,
      "shift": false,
      "line": {
        "type": "Talk",
        "values": [
          {
            "key": "voice",
