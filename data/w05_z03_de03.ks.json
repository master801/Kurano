      }
    },
    {
      "lineNumber": 699,
      "shift": false,
      "line": {
        "text": "「う、うんっ」\r\n"
      }
    },
    {
      "lineNumber": 705,
      "shift": false,
      "line": {
        "type": "Talk",
        "values": [
          {
            "key": "voice",
