    {
      "lineNumber": 719,
      "shift": false,
      "line": {
        "type": "Talk",
        "values": [
          {
            "key": "name",
            "value": "心の声"
          }
        ]
      }
    },
    {
      "lineNumber": 720,
      "shift": false,
      "line": {
        "text": "俺たち三人を見る姉さんの視線に気付かないふりをしつつ、\r\n"
      }
    },
    {
      "lineNumber": 721,
      "shift": false,
      "line": {
