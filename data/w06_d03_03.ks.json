    },
    {
      "lineNumber": 87,
      "line": "\t@char file=CD03001M\t\t\r\n"
    },
    {
      "lineNumber": 88,
      "line": "\t@tone all type=sepia\r\n"
    },
    {
      "lineNumber": 89,
      "line": "\t@update transition=mosaic maxsize=30 time=500\r\n"
    },
    {
      "lineNumber": 90,
      "line": "\t@waitUpdate\r\n"
    },
    {
      "lineNumber": 91,
