        {
            "line": {
                "text": "「——ああああっ？」\r\n"
            },
            "annotation": {},
            "shouldShift": false,
            "shift": 0,
            "textNumber": 15
        },
        {
            "line": {},
            "annotation": {
                "type": "Talk",
                "values": [
