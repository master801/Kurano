                    },
                    {
                        "voice": "MK001175"
                    }
                ]
            },
            "shouldShift": false,
            "shift": 0,
            "textNumber": 212
        },
        {
            "line": {
                "text": "「ばっちりいっぱいぬれたよね、カズくんっ」\r\n"
            },
            "annotation": {},
            "shouldShift": false,
            "shift": 0,
            "textNumber": 213
        },
        {
            "line": {},
            "annotation": {
                "type": "Talk",
                "values": [
                    {
                        "name": "和哉"
                    }
                ]
            },
            "shouldShift": false,
            "shift": 0,
            "textNumber": 216
        },
        {
            "line": {
                "text": "「う……うん……そうだな……」\r\n"
            },
            "annotation": {},
            "shouldShift": false,
            "shift": 0,
            "textNumber": 217
        },
        {
            "line": {},
            "annotation": {
                "type": "Talk",
                "values": [
                    {
                        "name": "心の声"
                    }
                ]
            },
            "shouldShift": false,
            "shift": 0,
            "textNumber": 220
        },
        {
            "line": {
