            },
            "annotation": {},
            "shouldShift": false,
            "shift": 0,
            "textNumber": 148
        },
        {
            "line": {},
            "annotation": {
                "type": "Talk",
                "values": [
                    {
                        "name": "心の声"
                    }
                ]
            },
            "shouldShift": false,
            "shift": 0,
            "textNumber": 151
        },
        {
            "line": {
