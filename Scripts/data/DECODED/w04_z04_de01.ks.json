                "type": "Talk",
                "values": [
                    {
                        "name": "絵麻"
                    },
                    {
                        "voice": "EM000653"
                    }
                ]
            },
            "shouldShift": false,
            "shift": 0,
            "textNumber": 190
        },
        {
            "line": {
                "text": "「はい」\r\n"
            },
            "annotation": {},
            "shouldShift": false,
            "shift": 0,
            "textNumber": 191
        },
        {
            "line": {},
            "annotation": {
                "type": "Talk",
                "values": [
                    {
                        "name": "弥恵"
                    },
                    {
                        "voice": "YE000769"
                    }
                ]
