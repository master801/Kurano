        },
        {
            "line": {},
            "annotation": {
                "type": "Talk",
                "values": [
                    {
                        "name": "和哉"
                    }
                ]
            },
            "shouldShift": false,
            "shift": 0,
            "textNumber": 198
        },
        {
            "line": {
