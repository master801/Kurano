            "annotation": {},
            "shouldShift": false,
            "shift": 0,
            "textNumber": 155
        },
        {
            "line": {},
            "annotation": {
                "type": "Talk",
                "values": [
                    {
                        "name": "弥恵"
                    },
                    {
                        "voice": "YE040705"
                    }
                ]
            },
            "shouldShift": false,
            "shift": 0,
            "textNumber": 161
        },
        {
