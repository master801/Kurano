            },
            "annotation": {},
            "shouldShift": false,
            "shift": 0,
            "textNumber": 150
        },
        {
            "line": {},
            "annotation": {
                "type": "Talk",
                "values": [
                    {
                        "name": "和哉"
                    }
                ]
            },
            "shouldShift": false,
            "shift": 0,
            "textNumber": 153
        },
        {
            "line": {
