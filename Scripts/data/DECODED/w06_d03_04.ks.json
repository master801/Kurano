            "shouldShift": false,
            "shift": 0,
            "textNumber": 142
        },
        {
            "line": {},
            "annotation": {
                "type": "Talk",
                "values": [
                    {
                        "name": "弥恵"
                    },
                    {
                        "voice": "YE040704"
                    }
                ]
            },
            "shouldShift": false,
            "shift": 0,
            "textNumber": 148
        },
        {
            "line": {
