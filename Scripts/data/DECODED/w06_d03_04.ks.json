            },
            "annotation": {},
            "shouldShift": false,
            "shift": 0,
            "textNumber": 135
        },
        {
            "line": {},
            "annotation": {
                "type": "Talk",
                "values": [
                    {
                        "name": "弥恵"
                    },
                    {
                        "voice": "YE040703"
                    }
                ]
            },
            "shouldShift": false,
            "shift": 0,
            "textNumber": 140
        },
        {
            "line": {
