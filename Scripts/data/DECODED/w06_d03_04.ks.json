        },
        {
            "line": {},
            "annotation": {
                "type": "Talk",
                "values": [
                    {
                        "name": "弥恵"
                    },
                    {
                        "voice": "YE040707"
                    }
                ]
            },
            "shouldShift": false,
            "shift": 0,
            "textNumber": 205
        },
        {
            "line": {
